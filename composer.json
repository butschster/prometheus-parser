{
    "name": "butschster/prometheus-parser",
    "description": "Prometheus parser written on PHP 8",
    "keywords": [
        "php8",
        "php",
        "prometheus",
        "ddl",
        "prometheus-parser"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Buchnev",
            "email": "butschster@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "phplrt/runtime": "^3.2"
    },
    "require-dev": {
        "phplrt/phplrt": "^3.2",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "Butschster\\Prometheus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Butschster\\Prometheus\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
